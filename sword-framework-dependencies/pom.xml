            </dependency>
            <!--mybatis插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-mybatis</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--OSS对象存储插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-oss</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--短信服务插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-sms</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--seata分布式事务-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-seata</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <dependency>
