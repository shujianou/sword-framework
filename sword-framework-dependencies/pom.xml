                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-i18n</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--mybatis插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-mybatis</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--短信服务插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-sms</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--任务调度插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-job</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--OSS插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-oss</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--redis插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-redis</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--excel插件-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-framework-starter-excel</artifactId>
                <version>${sword-framework.version}</version>
            </dependency>
            <!--公共核心模块-->
            <dependency>
                <groupId>com.sword.framework</groupId>
                <artifactId>sword-common-core</artifactId>
                <version>${sword-framework.version}</version>
